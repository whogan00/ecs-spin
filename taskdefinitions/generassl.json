[
   {
     "name": "nginx",
     "image": "nginx:1.19.7",
     "memory": 64,
     "cpu": 128,
     "essential": true,
     "logConfiguration": {
       "logDriver": "json-file",
       "options": {
         "max-size": "100m",
         "max-file": "2"
       }
     },
     "portMappings": [
       {
         "containerPort": 80,
         "protocol": "tcp"
       }
     ],
     "dockerLabels": {
        "com.datadoghq.ad.instances": "[{\"bucket\":\"datadog-healthcheck-bucket\", \"type\":\"eis_jks\", \"cluster\":\"test-cluster\",\"client\":\"cs\",\"environment\":\"dev\",\"region\": \"us-east-1\", \"min_collection_interval\": 3600}]",
        "com.datadoghq.ad.check_names": "[\"s3\"]",
        "com.datadoghq.ad.init_configs": "[{}]",
        "com.datadoghq.ad.logs": "[{\"source\": \"tomcat\", \"log_processing_rules\": [{\"type\": \"multi_line\", \"name\": \"log_start_with_date\", \"pattern\": \"\\\\d{2}-\\\\w{3}-\\\\d{4}|^Completed|^\\\\u001b\\\\[0|^TASK|^PLAY\"}]}]",
        "com.datadoghq.tags.service": "generalssb",
        "com.datadoghq.tags.env": "test",
        "com.datadoghq.tags.ecs_cluster": "test-cluster"
      }
   }
]
